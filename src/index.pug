doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    link(rel="stylesheet", href="./main.css")
    title Bookshop
  body 
    .wrapper 
      header.header
        .logo Bookshop

        - var linkList = ["books", "audiobooks", "Stationery & gifts", "blog"]
        ul.link-wrapper 
          each item in linkList
            li.link-wrapper__item
              a(href="#")= item


     
       